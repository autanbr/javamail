@model IEnumerable<Aula10.Models.Treinamento>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<p>
    @Html.ActionLink("Incluir", "Incluir")
</p>
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.Treinammento)</th>
        <th>@Html.DisplayNameFor(model => model.Inicio)</th>
        <th>@Html.DisplayNameFor(model => model.Termino)</th>
        <th>@Html.DisplayNameFor(model => model.Preco)</th>
        <th>@Html.DisplayNameFor(model => model.Desconto)</th>
        <th>@Html.DisplayNameFor(model => model.Local)</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.Treinammento)</td>
            <td>@Html.DisplayFor(modelItem => item.Inicio)</td>
            <td>@Html.DisplayFor(modelItem => item.Termino)</td>
            <td>@Html.DisplayFor(modelItem => item.Preco)</td>
            <td>@Html.DisplayFor(modelItem => item.Desconto)</td>
            <td>@Html.DisplayFor(modelItem => item.Local)</td>
            <td></td>
        </tr>
    }

</table>

